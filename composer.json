{
    "name": "fathihfaiz/laravel-maps-suite",
    "description": "Laravel Blade components for Leaflet, Google Maps, and Mapbox integration.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/fathihfaiz/laravel-maps-suite",
    "authors": [
      {
        "name": "Fathih Faiz",
        "email": "fathihfaiz@gmail.com"
      }
    ],
    "require": {
      "php": ">=8.0",
      "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
      "psr-4": {
        "FathihFaiz\\MapsSuite\\": "src/"
      }
    },
    "config": {
      "sort-packages": true,
      "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
      "laravel": {
        "providers": [
          "FathihFaiz\\MapsSuite\\MapsSuiteServiceProvider"
        ],
        "aliases": {
          "MapsSuite": "FathihFaiz\\\\MapsSuite\\\\MapsSuiteFacade"
        }
      }
    }
  }